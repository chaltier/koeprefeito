# Environment variables for local development
# Copy this file to .env.local and fill in your values

# Next.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-here-generate-a-random-string-min-32-chars

# Google OAuth (Get from Google Cloud Console)
# OPTIONAL: Leave empty to test without OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Database (Default for Docker setup)
DATABASE_URL=postgresql://koeprefeito:password@localhost:5432/koeprefeito

# Redis (Optional for caching)
REDIS_URL=redis://localhost:6379

# Skip environment validation during development
# Set to 1 if you want to test without Google OAuth
SKIP_ENV_VALIDATION=1